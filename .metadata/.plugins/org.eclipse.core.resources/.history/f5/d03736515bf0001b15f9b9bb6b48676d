package coreJavaDemoSessionTwo;

public class Ex1 {
int x=20;//non static stores only in object without obj we cant accses it.
static int  y =50;//static member only class common memory it gets stored and we call it by using class name.
public static void main(String[] args) {
	Ex1 ex= new Ex1();
	System.out.println(Ex1.y);//reading static member
	System.out.println(ex.x);//reading non-static member
}
}
